    "export-db": "bun run scripts/export_database.ts",
    "amm:run": "bun run scripts/amm/http-run.ts",
    "amm:run-internal": "bun run scripts/amm/index.ts",
    "amm:test": "bun run scripts/amm/http-run.ts"
  },
  "dependencies": {
    "@clerk/backend": "^1.27.0",
    "dotenv": "^17.2.3",
    "encore.dev": "^1.53.4",
    "ethers": "^6"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  }
}